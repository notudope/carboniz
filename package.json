{
    "name": "carboniz",
    "version": "0.0.1",
    "description": "Carbon REST API.",
    "private": true,
    "main": "api/index.js",
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node api/index.js",
        "dev": "nodemon api/index.js",
        "format": "prettier --check \"./**/*.{js,json,yml,html,css}\" --write"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": false,
        "bracketSpacing": false,
        "bracketSameLine": true,
        "arrowParens": "always",
        "trailingComma": "all",
        "semi": true,
        "proseWrap": "always"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/notudope/carboniz.git"
    },
    "author": "notudope",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/notudope/carboniz/issues"
    },
    "homepage": "https://github.com/notudope/carboniz#readme",
    "dependencies": {
        "chrome-aws-lambda": "^8.0.2",
        "formidable": "^2.0.1",
        "prettier": "^2.6.0",
        "puppeteer-core": "^8.0.0",
        "raw-body": "^2.4.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.15"
    }
}
